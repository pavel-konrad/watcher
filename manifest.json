{
  "manifest_version": 3,
  "name": "Watcher",
  "version": "3.0",
  "description": "Your personal procrastination killer",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
 
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "./popup/popup.html"
  },
  "background": {
    "service_worker": "./background.js"
  }
}
